{"full_text":"${fs_used /} /${fs_size /}","color":"\#ebdbb2","separator":false,"separator_block_width":6},\
{"full_text":"HDD ","color":"\#a89984","separator":false,"separator_block_width":0},\
{"full_text":"${fs_used /media/nfs} / ${fs_size /media/nfs}","color":"\#ebdbb2","separator":false,"separator_block_width":6},\

